<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    app:startDestination="@id/forgetPasswordFragment3">
    <fragment
        android:id="@+id/forgetPasswordFragment3"
        android:name="com.cedricapp.fragment.ForgetPasswordFragment"
        android:label="activity_forget_password"
        tools:layout="@layout/activity_forget_password">
        <action
            android:id="@+id/action_forgetPasswordFragment3_to_otpFragment2"
            app:destination="@id/otpFragment2" />
    </fragment>
    <fragment
        android:id="@+id/otpFragment2"
        android:name="com.cedricapp.fragment.OtpFragment"
        android:label="fragment_otp"
        tools:layout="@layout/fragment_otp">
        <action
            android:id="@+id/action_otpFragment2_to_resetPasswordFragment2"
            app:destination="@id/resetPasswordFragment2" />
    </fragment>
    <fragment
        android:id="@+id/resetPasswordFragment2"
        android:name="com.cedricapp.fragment.ResetPasswordFragment"
        android:label="fragment_reset_password"
        tools:layout="@layout/fragment_reset_password" />
</navigation>